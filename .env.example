# ===============
# RADIUSdesk config
RADIUS_DESK_USERNAME=<USERNAME>
RADIUS_DESK_PASSWORD=<PASSWORD>
# Celery config
CELERY_BROKER_URL="redis://redis:6379/0"
# ===============
# 1FourYou Payment Config (v4 API)
# Environment: "sandbox" or "production" (default: "sandbox")
ONEFORYOU_ENVIRONMENT=sandbox
# API key is base64-encoded (as provided by Flash Integrations)
# Sandbox example: eUQzdG9QZW8xQ2RSWjcwWm5lamY1RF9zU0FVYTpoM0o0S0hHdTE5S1JkMDVFOUR5eVA1Y2dUOVlh
ONEFORYOU_API_KEY=your_base64_encoded_api_key
ONEFORYOU_ACCOUNT_NUMBER=your_account_number
# Legacy support (optional - will be converted if ONEFORYOU_API_KEY not set)
# ONEFORYOU_CONSUMER_KEY=your_consumer_key
# ONEFORYOU_CONSUMER_SECRET=your_consumer_secret
ONEFORYOU_USER_ID=XXXX
# ===============
# Traefik Configuration
TRAEFIK_BACKEND_HOST="backend.inethicloud.net"
TRAEFIK_ENTRYPOINTS="websecure"
TRAEFIK_CERTRESOLVER="letsencrypt"
TRAEFIK_NETWORK_BRIDGE="inethi-bridge-traefik"

SUPERUSER_USERNAME=<USERNAME>
SUPERUSER_PASSWORD=<PASSWORD>